# Environment Variables Template
# Copy this to .env.local and fill in your values

# API Configuration
# Replace with your Cloudflare Worker URL after deployment
VITE_API_URL=http://localhost:8787/api

# For production, use your deployed worker URL:
# VITE_API_URL=https://semrank-api.YOUR-SUBDOMAIN.workers.dev/api

# Cloudflare D1 (for reference)
# VITE_CLOUDFLARE_D1_ID=your-db-id

# Analytics (optional)
VITE_ANALYTICS_ID=

# Feature flags
VITE_ENABLE_GAME=true
VITE_ENABLE_COMPARE=true
